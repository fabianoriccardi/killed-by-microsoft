{
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.markdownlint": "explicit"
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/tmp": true,
    "**/dist": true
  },
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "TSX",
    "JSX"
  ],
  "typescript.tsdk": "node_modules/typescript/lib",
  "stm32-for-vscode.openOCDPath": false,
  "stm32-for-vscode.armToolchainPath": false
}